      "cell_type": "code",
      "source": [
        "df = pd.DataFrame({'A': [1, 2], 'B': [0.5, 0.75]},   index=['a', 'b'])\n",
        "df.to_records()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "IQDMm7m5yFmV",
        "outputId": "2ea9869d-e6e3-4410-eca6-564682a5a039"
